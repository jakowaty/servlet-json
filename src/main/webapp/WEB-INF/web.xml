<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <filter>
    <filter-name>JspFilter</filter-name>
    <filter-class>com.jakowaty.piotrbe.filter.JspFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>JspFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  
  <servlet>
  	<servlet-name>JakowatyFrontController</servlet-name>
  	<display-name>JakowatyFrontController</display-name>
  	<description></description>
  	<servlet-class>com.jakowaty.piotrbe.servlet.FrontController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>JakowatyFrontController</servlet-name>
  	<url-pattern>/app/*</url-pattern>
  </servlet-mapping>
  
</web-app>
